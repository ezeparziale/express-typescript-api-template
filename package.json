{
  "name": "express-typescript-api-template",
  "version": "1.0.0",
  "description": "Api template with express and typescript",
  "main": "server.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "ts-node-dev server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "@ezeparziale",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/morgan": "1.9.4",
    "@types/node": "20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "13.7.17",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "eslint": "8.42.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "dotenv": "16.1.4",
    "express": "4.18.2",
    "joi": "17.9.2",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "pg": "8.11.0",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.32.0",
    "sequelize-typescript": "2.1.5",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/tests/jest.setup.js"
    ]
  }
}
